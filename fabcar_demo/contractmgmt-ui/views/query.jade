extends layout

block content
    h2 Click to view/Edit Contracts
    #data
        if data != undefined && data.length > 0
            p.lead The returned result was: #{data}
        else
            a.btn.btn-lg.btn-primary(href='/actionQuery') Query Contracts
            a.btn.btn-lg.btn-primary(href='/display') Display
script(src='/bootstrap/js/bootstrap.bundle.js')

